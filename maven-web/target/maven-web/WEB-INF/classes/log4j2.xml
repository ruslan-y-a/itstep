<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%date{HH:mm:ss.SSS} %-5level [-10%thread] %class.%method():%line %msg%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="warn">  
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>
